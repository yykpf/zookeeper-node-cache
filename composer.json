{
    "name": "yykpf/zookeeper-node-cache",
    "description": "Command line cache of zookeeper node",
    "type": "library",
    "license": "MIT",
    "require": {
        "predis/predis": "^1.1",
        "php": ">=7.0.0"
//        "ext-zookeeper": ">=0.2.0"
    },
    "autoload": {
        "psr-4": {
            "ZookeeperNodeCache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZookeeperNodeCache\\Debugbar\\ServiceProvider"
            ],
            "aliases": {
                "zk": "Barryvdh\\Debugbar\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}